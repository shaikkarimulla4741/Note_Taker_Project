<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<display-name>SaveNoteServlet</display-name>
  	<description></description>
  	<servlet-class>hibernete.noteTaker.servlet.SaveNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<display-name>DeleteNoteServlet</display-name>
  	<description></description>
  	<servlet-class>hibernate.noteTaker.deleteNote.DeleteNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateNoteServlet</servlet-name>
  	<display-name>UpdateNoteServlet</display-name>
  	<description></description>
  	<servlet-class>hibernate.noteTaker.updateNote.UpdateNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdatingNoteServlet</servlet-name>
  	<display-name>UpdatingNoteServlet</display-name>
  	<description></description>
  	<servlet-class>hibernate.noteTaker.updateNote.UpdatingNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<url-pattern>/SaveNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<url-pattern>/DeleteNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateNoteServlet</servlet-name>
  	<url-pattern>/UpdateNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdatingNoteServlet</servlet-name>
  	<url-pattern>/UpdatingNoteServlet</url-pattern>
  </servlet-mapping>
</web-app>
